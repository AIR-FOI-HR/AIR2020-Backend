{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Event",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Name": {
      "type": "string"
    },
    "calendar": {
      "type": "component",
      "repeatable": true,
      "component": "hours.event-hours",
      "required": true
    },
    "event_type": {
      "model": "event-type"
    },
    "subject": {
      "model": "subject"
    },
    "room": {
      "model": "room"
    },
    "user": {
      "via": "events",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}

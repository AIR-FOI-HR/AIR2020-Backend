{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Event",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Name": {
      "type": "string"
    },
    "event_type": {
      "model": "event-type"
    },
    "subject": {
      "model": "subject"
    },
    "room": {
      "model": "room"
    },
    "user": {
      "via": "events",
      "plugin": "users-permissions",
      "model": "user"
    },
    "day": {
      "type": "enumeration",
      "enum": [
        "monday",
        "tuesday",
        "wedensday",
        "thursday",
        "friday"
      ],
      "required": true,
      "default": "monday"
    },
    "start_time": {
      "type": "string",
      "required": true
    },
    "end_time": {
      "type": "string",
      "required": true
    }
  }
}
